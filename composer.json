{
    "name": "jazzman/htaccess",
    "license": "GPL-3.0",
    "type": "wordpress-muplugin",
    "description": "",
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "authors": [
        {
            "name": "Vasyl Sokolyk",
            "email": "vsokolyk@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Jazz-Man/htaccess/issues"
    },
    "require": {
        "php": ">=5.6",
        "ralouphie/mimey": "^2.0",
        "tivie/htaccess-parser": "^0.2.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^4.2",
        "dg/composer-cleaner": "^2.0"
    },
    "extra":{
        "cleaner-ignore":{
            "ralouphie/mimey":[
                "mime.*"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "JazzMan\\Htaccess\\":"src"
        }
    }
}
