{
    "name": "jazzman/htaccess",
    "license": "GPL-3.0",
    "version": "1.0",
    "type": "wordpress-muplugin",
    "description": "Boilerplate's .htaccess for WordPress",
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "keywords": [
        "wordpress",
        "htaccess"
    ],
    "authors": [
        {
            "name": "Vasyl Sokolyk",
            "email": "vsokolyk@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Jazz-Man/htaccess/issues"
    },
    "require": {
        "php": ">=5.6",
        "jazzman/autoload-interface": "^0.1.0",
        "jazzman/singleton-trait": "^1.1",
        "tivie/htaccess-parser": "^0.2.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "JazzMan\\Htaccess\\":"src"
        },
        "files": [
            "helper.php"
        ]
    }
}
